/*
 * This is an example usage of a do_while loop in the comp25 language
 */
program main
begin 
    procedure main()
    begin
        var x: int
    in
        x := 0;
        do 
            print_int(x);
            print_string("\n");
            x := x + 1;
            exit when x >= 10
        od 
    end
end

